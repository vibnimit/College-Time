<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <gradient android:centerX="50%" android:centerY="50%" android:endColor="#516a79" android:gradientRadius="85" android:startColor="#294657" android:type="radial" />

            <stroke android:width="2dp" android:color="#DDDDFF" />
        </shape></item>
    <item><shape>
            <solid android:color="#516a79" />

            <stroke android:width="2dp" android:color="#9bb7c8" />
        </shape></item>

</selector>